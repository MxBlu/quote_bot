{
  "name": "quote_bot",
  "version": "2.0.0",
  "description": "Quote messages and store quotes",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext ts,json --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "fstart": "pm2 start --name quotebot built/main.js",
    "build": "tsc --build tsconfig.json",
    "clean": "tsc --build --clean",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@discordjs/builders": "0.6.0",
    "@discordjs/rest": "0.1.0-canary.0",
    "@typegoose/auto-increment": "MxBlu/auto-increment#0.9.1",
    "@typegoose/typegoose": "8.2.0",
    "apollo-server": "3.5.0",
    "apollo-server-express": "3.5.0",
    "bot-framework": "Mxblu/bot-framework#4.3.8",
    "bufferutil": "4.0.3",
    "class-validator": "0.13.2",
    "cookie": "0.4.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "discord-api-types": "0.22.0",
    "discord.js": "13.1.0",
    "dotenv": "8.0.0",
    "express": "5.0.0-alpha.8",
    "graphql": "15.3.0",
    "mongoose": "5.13.8",
    "node-fetch": "3.1.0",
    "reflect-metadata": "0.1.13",
    "ts-node": "^10.1.0",
    "type-graphql": "1.1.1",
    "utf-8-validate": "5.0.5",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@types/cookie": "0.4.1",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "17.0.4",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.28.0",
    "nodemon": "1.19.0",
    "typescript": "4.3.5"
  }
}
